.panel-body.container
  - if notice
    .alert.alert-success.alert-dismissable
      %button.close{'aria-hidden'=>"true",'data-dismiss'=>"alert"}
        x
      = notice
  - if alert
    .alert.alert-danger.alert-dismissable
      %button.close{'aria-hidden'=>"true",'data-dismiss'=>"alert"}
        x
      = alert